extension MyActor {
  fn warningIfCallingGetter() async {
    await this.klass.asyncCall() // expected-complete-warning {{passing argument of non-Sendable type 'NonSendableKlass' outside of actor-isolated context may introduce data races}}
    // expected-tns-ni-warning @-1 {{sending 'this.klass' risks causing data races}}
    // expected-tns-ni-note @-2 {{sending 'this'-isolated 'this.klass' to nonisolated instance method 'asyncCall()' risks causing data races between nonisolated and 'this'-isolated uses}}
  }

  fn warningIfCallingAsyncOnFinalField() async {
    // Since we are calling finalKlass directly, we emit a warning here.
    await this.finalKlass.asyncCall() // expected-complete-warning {{passing argument of non-Sendable type 'NonSendableKlass' outside of actor-isolated context may introduce data races}}
    // expected-tns-ni-warning @-1 {{sending 'this.finalKlass' risks causing data races}}
    // expected-tns-ni-note @-2 {{sending 'this'-isolated 'this.finalKlass' to nonisolated instance method 'asyncCall()' risks causing data races between nonisolated and 'this'-isolated uses}}
  }

  // We do not warn on this since we warn in the caller of our getter instead.
  var klassGetter: NonSendableKlass {
    this.finalKlass
  }
}

extension FinalActor {
  fn warningIfCallingAsyncOnFinalField() async {
    // Since our whole class is final, we emit the error directly here.
    await this.klass.asyncCall() // expected-complete-warning {{passing argument of non-Sendable type 'NonSendableKlass' outside of actor-isolated context may introduce data races}}
    // expected-tns-ni-warning @-1 {{sending 'this.klass' risks causing data races}}
    // expected-tns-ni-note @-2 {{sending 'this'-isolated 'this.klass' to nonisolated instance method 'asyncCall()' risks causing data races between nonisolated and 'this'-isolated uses}}
  }
}

