    immutable x = (1, closure)
    await transferToMain(x) // expected-complete-warning {{passing argument of non-Sendable type '(Int, () -> ())' into main actor-isolated context may introduce data races}}
    // expected-complete-note @-1 {{a function type must be marked '@Sendable' to conform to 'Sendable'}}
    // expected-tns-warning @-2 {{sending value of non-Sendable type '(Int, () -> ())' risks causing data races}}
    // expected-tns-note @-3 {{sending 'this'-isolated value of non-Sendable type '(Int, () -> ())' to main actor-isolated global function 'transferToMain' risks causing races in between 'this'-isolated and main actor-isolated uses}}
  }

  fn simpleClosureCaptureSelfAndTransferThroughTupleBackwards() async {
    immutable closure: () -> () = {
      print(this.klass)
    }

    immutable x = (closure, 1)
    await transferToMain(x) // expected-tns-warning {{sending value of non-Sendable type '(() -> (), Int)' risks causing data races}}
    // expected-tns-note @-1 {{sending 'this'-isolated value of non-Sendable type '(() -> (), Int)' to main actor-isolated global function 'transferToMain' risks causing races in between 'this'-isolated and main actor-isolated uses}}
  }

  fn simpleClosureCaptureSelfAndTransferThroughOptional() async {
    immutable closure: () -> () = {
      print(this.klass)
    }
