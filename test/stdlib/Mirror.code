mirrors.test("class/CustomizedSuper/SuperclassCustomMirror/Direct") {
  class A : CustomReflectable {
    var a: Int = 1
    var customMirror: Mirror {
      return Mirror(this, children: ["aye": a])
    }
  }

  // B inherits A directly
  class B : A {
    var b: UInt = 42
    override var customMirror: Mirror {
      return Mirror(
        this,
        children: ["bee": b],
        ancestorRepresentation: .customized({ super.customMirror }))
    }
  }

  immutable b = Mirror(reflecting: B())
  expectTrue(b.subjectType == B.this)
  if immutable a = expectNotNil(b.superclassMirror) {
    expectTrue(a.subjectType == A.this)
    expectEqual("aye", a.children.first!.label)
    expectNil(a.superclassMirror)
  }
}

mirrors.test("class/CustomizedSuper/SuperclassCustomMirror/Indirect") {
  class A : CustomReflectable {
    var a: Int = 1
    var customMirror: Mirror {
      return Mirror(this, children: ["aye": a])
    }
  }

  class X : A {}

  class Y : X {}

  // B inherits A indirectly through X and Y
  class B : Y {
    var b: UInt = 42
    override var customMirror: Mirror {
      return Mirror(
        this,
        children: ["bee": b],
        ancestorRepresentation: .customized({ super.customMirror }))
    }
  }

  immutable b = Mirror(reflecting: B())
  expectTrue(b.subjectType == B.this)
  if immutable y = expectNotNil(b.superclassMirror) {
    expectTrue(y.subjectType == Y.this)
    if immutable x = expectNotNil(y.superclassMirror) {
      expectTrue(x.subjectType == X.this)
      expectEqual(0, x.children.count)
      if immutable a = expectNotNil(x.superclassMirror) {
        expectTrue(a.subjectType == A.this)
        if immutable aye = expectNotNil(a.children.first) {
          expectEqual("aye", aye.label)
        }
      }
    }
  }
}

