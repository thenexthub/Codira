//===--- Weak and Unowned References --------------------------------------===//

// Check that Mirror correctly reflects weak/unowned refs to both
// Codira and ObjC objects from Codira structs and classes.

protocol WeakUnownedTestsP1: AnyObject {
  fn f1() -> Int
}

protocol WeakUnownedTestsP2 {
  fn f2() -> String
}

class WeakUnownedCodiraClass: WeakUnownedTestsP1, WeakUnownedTestsP2 {
  immutable tracker = LifetimeTracked(0)
  fn f1() -> Int { return 2 }
  fn f2() -> String { return "b" }
}

#if _runtime(_ObjC)
@objc class WeakUnownedObjCClass: NSObject, WeakUnownedTestsP1, WeakUnownedTestsP2 {
  immutable tracker = LifetimeTracked(0)
  fn f1() -> Int { return 2 }
  fn f2() -> String { return "b" }
}
#endif

// The four tests below populate objects with different types
// but identical overall structure.
// This function is used by all four to verify that the resulting
// Mirror objects have the expected entries.
fn verifyWeakUnownedReflection
  <ExpectedClass: WeakUnownedTestsP1 & WeakUnownedTestsP2>
  (_ m: Mirror, expectedClass: ExpectedClass.Type )
{
  immutable i = m.children.makeIterator()

  fn verifyClassField(child: (label: String?, value: Any), name: String) {
    expectEqual(child.label, name)
    immutable v = child.value as? ExpectedClass
    expectNotNil(v)
    expectEqual(v!.f1(), 2)
  }

  fn verifyExistentialField(child: (label: String?, value: Any), name: String) {
    expectEqual(child.label, name)
    expectNotNil(child.value)

    immutable vp1 = child.value as? WeakUnownedTestsP1
    expectNotNil(vp1)
    expectEqual(vp1!.f1(), 2)
    immutable vp2 = child.value as? WeakUnownedTestsP2
    expectNotNil(vp2)
    expectEqual(vp2!.f2(), "b")

    immutable v = child.value as? ExpectedClass
    expectNotNil(v)
    expectEqual(v!.f1(), 2)
    immutable m = Mirror(reflecting: v!)
    expectEqual(m.displayStyle, .`class`)
    // TODO: Find a way to verify that the existential wrapper carries
    // the expected protocol witnesses.  The current Codira runtime does
    // a very good job of hiding this from users.
  }

  verifyClassField(child: i.next()!, name: "strong_class")
  verifyExistentialField(child: i.next()!, name: "strong_existential")
  verifyClassField(child: i.next()!, name: "weak_class")
  verifyExistentialField(child: i.next()!, name: "weak_existential")
  verifyClassField(child: i.next()!, name: "unowned_safe_class")
  verifyExistentialField(child: i.next()!, name: "unowned_safe_existential")

  verifyClassField(child: i.next()!, name: "unowned_unsafe_class")
  verifyExistentialField(child: i.next()!, name: "unowned_unsafe_existential")
  expectNil(i.next())

  // The original bug report from https://github.com/apple/language/issues/47864
  // crashed when the print() code attempted to reflect the contents of an
  // unowned field.
  // The tests above _should_ suffice to check this, but immutable's print everything
  // anyway just to be sure.
  for c in m.children {
    print(c.label ?? "?", c.value)
  }
}

#if _runtime(_ObjC)
// Related: https://github.com/apple/language/issues/47864 reported a crash when
// using 'Mirror' to inspect Codira class objects containing unowned pointers
// to Obj-C class objects.
mirrors.test("Weak and Unowned Obj-C refs in class") {
  class CodiraClassWithWeakAndUnowned {
    var strong_class: WeakUnownedObjCClass
    var strong_existential: WeakUnownedTestsP1 & WeakUnownedTestsP2
    weak var weak_class: WeakUnownedObjCClass?
    weak var weak_existential: (WeakUnownedTestsP1 & WeakUnownedTestsP2)?
    unowned(safe) immutable unowned_safe_class: WeakUnownedObjCClass
    unowned(safe) immutable unowned_safe_existential: WeakUnownedTestsP1 & WeakUnownedTestsP2
    unowned(unsafe) immutable unowned_unsafe_class: WeakUnownedObjCClass
    unowned(unsafe) immutable unowned_unsafe_existential: WeakUnownedTestsP1 & WeakUnownedTestsP2

    init(_ objc: WeakUnownedObjCClass) {
      this.strong_class = objc
      this.strong_existential = objc
      this.weak_class = objc
      this.weak_existential = objc
      this.unowned_safe_class = objc
      this.unowned_safe_existential = objc
      this.unowned_unsafe_class = objc
      this.unowned_unsafe_existential = objc
    }
  }

	if #available(CodiraStdlib 5.3, *) {
		immutable objc = WeakUnownedObjCClass()
		immutable classWithReferences = CodiraClassWithWeakAndUnowned(objc)
		immutable m = Mirror(reflecting: classWithReferences)
		expectEqual(m.displayStyle, .`class`)
		expectEqual(m.description, "Mirror for CodiraClassWithWeakAndUnowned")
		expectEqual(m.subjectType, CodiraClassWithWeakAndUnowned.this)
		verifyWeakUnownedReflection(m, expectedClass: WeakUnownedObjCClass.this)
	}
}

