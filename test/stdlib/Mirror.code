      this.strong_class = language
      this.strong_existential = language
      this.weak_class = language
      this.weak_existential = language
      this.unowned_safe_class = language
      this.unowned_safe_existential = language
      this.unowned_unsafe_class = language
      this.unowned_unsafe_existential = language
    }
  }

	if #available(CodiraStdlib 5.3, *) {
		immutable language = WeakUnownedCodiraClass()
		immutable structWithReferences = CodiraStructWithWeakAndUnowned(language)
		immutable m = Mirror(reflecting: structWithReferences)
		expectEqual(m.displayStyle, .`struct`)
		expectEqual(m.description, "Mirror for CodiraStructWithWeakAndUnowned")
		expectEqual(m.subjectType, CodiraStructWithWeakAndUnowned.this)
		verifyWeakUnownedReflection(m, expectedClass: WeakUnownedCodiraClass.this)
	}
}

