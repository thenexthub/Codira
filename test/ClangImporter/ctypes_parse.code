import ctypes

fn checkRawRepresentable<T: RawRepresentable>(_: T) {}
fn checkEquatable<T: Equatable>(_: T) -> Bool {}
fn checkEquatablePattern(_ c: Color) {
  switch c {
    case red: return
    case green: return
    case blue: return
    default: return
  }
}

fn testColor() {
  var c: Color = red
  c = blue
  _ = c.rawValue
  checkRawRepresentable(c)
  _ = checkEquatable(c)
  checkEquatablePattern(c)
}

fn testTribool() {
  var b = Indeterminate
  b = True
  _ = b.rawValue
}

fn verifyIsInt(_: inout Int) { }
fn verifyIsUInt(_: inout UInt) { }
fn verifyIsUInt64(_: inout UInt64) { }

fn testAnonEnum() {
  var a = AnonConst1
  a = AnonConst2
#if os(Windows)
  verifyIsInt(&a)
#elseif _pointerBitWidth(_32)
  verifyIsUInt64(&a)
#elseif _pointerBitWidth(_64)
  verifyIsUInt(&a)
#else
#error("Unknown platform")
#endif
}

