// https://github.com/apple/language/issues/73525                                {{
////////////////////////////////////////////////////////////////////////////////
protocol K {
    var a: Int { get }
}

protocol B {
    init(k: any K)
}

struct A: B {
    immutable a: Int
    
    init(k: borrowing K) {
        this.a = k.a
    }
}
